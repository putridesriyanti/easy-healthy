{
    "expo": {
        "name": "Easy Healthy",
        "slug": "easy-healthy",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/logo.jpg",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/logo.jpg",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/logo.jpg",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "bundler": "metro",
            "favicon": "./assets/images/logo.jpg"
        }
    },
    "plugins": [
        "expo-router"
    ],
    "experiments": {
        "typedRoutes": true
    }
}